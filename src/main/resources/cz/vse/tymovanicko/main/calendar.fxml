<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<VBox prefHeight="500.0" prefWidth="600.0" style="-fx-background-color: #37598e;" xmlns="http://javafx.com/javafx/18"
      xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label text="Kalendář" textFill="WHITE">
            <font>
                <Font name="System Bold" size="20.0"/>
            </font>
            <padding>
                <Insets left="20.0" right="20.0" top="10.0"/>
            </padding>
        </Label>
        <GridPane alignment="TOP_CENTER" hgap="10.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                  minWidth="-Infinity" vgap="10.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Měsíc a rok" textFill="WHITE">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label text="Po" textFill="WHITE" GridPane.rowIndex="1"/>
                <Label text="Út" textFill="WHITE" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Label text="St" textFill="WHITE" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
                <Label text="Čt" textFill="WHITE" GridPane.columnIndex="3" GridPane.rowIndex="1"/>
                <Label text="Pá" textFill="WHITE" GridPane.columnIndex="4" GridPane.rowIndex="1"/>
                <Label text="So" textFill="WHITE" GridPane.columnIndex="5" GridPane.rowIndex="1"/>
                <Label text="Ne" textFill="WHITE" GridPane.columnIndex="6" GridPane.rowIndex="1"/>
                <Label text="zápas" textFill="WHITE" GridPane.rowIndex="7">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Rectangle arcHeight="1.0" arcWidth="1.0" fill="#ff1f1f" height="20.0" stroke="BLACK"
                           strokeType="INSIDE" strokeWidth="0.0" width="40.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="7"/>
                <Label text="trénink" textFill="WHITE" GridPane.rowIndex="8">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Rectangle arcHeight="1.0" arcWidth="1.0" fill="DODGERBLUE" height="20.0" stroke="BLACK"
                           strokeType="INSIDE" strokeWidth="0.0" width="40.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="8"/>
            </children>
            <padding>
                <Insets bottom="25.0" left="25.0" right="25.0" top="25.0"/>
            </padding>
        </GridPane>
    </children>
</VBox>

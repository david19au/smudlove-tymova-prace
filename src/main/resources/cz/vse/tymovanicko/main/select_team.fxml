<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0"
      prefWidth="600.0" spacing="5.0" style="-fx-background-color: #37598e;" xmlns="http://javafx.com/javafx/18"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.vse.tymovanicko.main.SelectTeamController">
    <children>
        <Label alignment="CENTER" text="Do kterého týmu se chcete přihlásit?" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="System Bold" size="20.0"/>
            </font>
        </Label>
        <GridPane alignment="CENTER" hgap="10.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                  minWidth="-Infinity" prefHeight="300.0" prefWidth="450.0" vgap="10.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
            <children>
                <Button alignment="CENTER" mnemonicParsing="false" text="Tým 1" textAlignment="CENTER">
                    <font>
                        <Font name="System Bold" size="20.0"/>
                    </font>
                    <GridPane.margin>
                        <Insets bottom="30.0" left="30.0" top="30.0"/>
                    </GridPane.margin>
                    <padding>
                        <Insets bottom="30.0" left="50.0" right="50.0" top="30.0"/>
                    </padding>
                </Button>
                <Button alignment="CENTER" mnemonicParsing="false" text="Tým 2" textAlignment="CENTER"
                        GridPane.columnIndex="1">
                    <padding>
                        <Insets bottom="30.0" left="50.0" right="50.0" top="30.0"/>
                    </padding>
                    <font>
                        <Font name="System Bold" size="20.0"/>
                    </font>
                    <GridPane.margin>
                        <Insets bottom="30.0" left="10.0" right="30.0" top="30.0"/>
                    </GridPane.margin>
                </Button>
                <Button alignment="CENTER" mnemonicParsing="false" text="Tým 3" textAlignment="CENTER"
                        GridPane.rowIndex="1">
                    <padding>
                        <Insets bottom="30.0" left="50.0" right="50.0" top="30.0"/>
                    </padding>
                    <font>
                        <Font name="System Bold" size="20.0"/>
                    </font>
                    <GridPane.margin>
                        <Insets bottom="30.0" left="30.0" top="30.0"/>
                    </GridPane.margin>
                </Button>
                <Button alignment="CENTER" mnemonicParsing="false" text="Tým 4" textAlignment="CENTER"
                        GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <padding>
                        <Insets bottom="30.0" left="50.0" right="50.0" top="30.0"/>
                    </padding>
                    <font>
                        <Font name="System Bold" size="20.0"/>
                    </font>
                    <GridPane.margin>
                        <Insets bottom="30.0" left="10.0" right="30.0" top="30.0"/>
                    </GridPane.margin>
                </Button>
            </children>
            <VBox.margin>
                <Insets bottom="10.0" left="40.0" right="10.0" top="50.0"/>
            </VBox.margin>
        </GridPane>
    </children>
    <padding>
        <Insets bottom="30.0" left="30.0" right="30.0" top="30.0"/>
    </padding>
</VBox>
